        // "python": "set PYTHONIOENCODING=utf8 && cd $workspaceRoot && \"D:/Program Files/Python/Python392/python\" -u $fullFileName",
    },
    "code-runner.saveFileBeforeRun": true,
    "code-runner.runInTerminal": true,
    
}